<h1>Performances#new</h1>
<p>Find me in app/views/performances/new.html.erb</p>
<%= form_for @performance do  |f|%>
    <%= f.hidden_field :show_id, value: @show.id %>
    <%= f.label :date %>
    <%= f.text_field :date %>
    <%= f.label "Venue", :venue_id %>
        <%= f.select :venue_id, @venues %>
    <%= f.submit %>
<% end %>
