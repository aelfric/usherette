<%= form_for @performance do  |f|%>
    <%= f.hidden_field :show_id, value: @show.id %>
    <div class='field'>
    <%= f.label :date %>
    <%= f.text_field :date %>
</div>
<div class='field'>
    <%= f.label :venue_id , "Venue"%>
    <%= f.select :venue_id, @venues.map{ |v| v.name } %>
</div>
<div class='field'>
        <%= f.label :price,  "Ticket Price (in cents)" %>
        <%= f.text_field :price %>
    </div>
    <%= f.submit %>
<% end %>
