<table border=1>
    <% @cart.tickets.each do |t| %>
        <tr>
            <td><%= t.performance.date_string %></td>
            <td><%= t.performance.show.title %></td>
            <td><%= t.performance.price_string %></td>
        </tr>
    <% end %>
</table>
<%= link_to 'Begin Checkout', edit_cart_path(@current_cart) %>
<%= link_to "Checkout", @current_cart.paypal_url(performances_url, payment_notifications_url) %>
